<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
  <DataTemplate x:Key="ChartPanelMoreIndicatorPanel_DataTemplate" >
    <Border BorderBrush="Black" BorderThickness="1" CornerRadius="5" Margin="1">
      <StackPanel>
        <TextBlock Text="{Binding Name}" FontSize="12" HorizontalAlignment="Center" Margin="1" Foreground="Blue"/>
        <TextBlock Text="{Binding Value}" FontSize="12" HorizontalAlignment="Left" FontWeight="Bold" Margin="1" Foreground="Navy"/>
      </StackPanel>
    </Border>
  </DataTemplate>
  
</ResourceDictionary>